{% extends 'base.html.twig' %}

{% block title %}Tous les acteurs!{% endblock %}

{% block body %}
    <h1>Tous les acteurs : </h1>
    <ul>
    {% for actor in actors %}
        <li>
            <a href="{{ path('actor_show', {'id':actor.id}) }}">{{ actor.name }}</a>
        </li>
    {% endfor%}
    </ul>
{% endblock %}
